<p id="notice"><%= notice %></p>

<h1>Listing Predictions</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Trump ec votes</th>
      <th>Clinton ec votes</th>
      <th>Lockin datetime</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @predictions.each do |prediction| %>
      <tr>
        <td><%= prediction.first_name %></td>
        <td><%= prediction.last_name %></td>
        <td><%= prediction.trump_ec_votes %></td>
        <td><%= prediction.clinton_ec_votes %></td>
        <td><%= prediction.lockin_datetime %></td>
        <td><%= link_to 'Show', prediction %></td>
        <td><%= link_to 'Edit', edit_prediction_path(prediction) %></td>
        <td><%= link_to 'Destroy', prediction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Prediction', new_prediction_path %>
